      </nav>

      <div class="container">
        <h5 style="margin: 20px; text-align: center;"> Analysis </h5>
    </div>




    <div style="width: 30%; height: 30%; margin-left: 35%; margin-top: 5%;">

        <canvas id="myChart"></canvas>
      </div>

      <script src="https://cdn.jsdelivr.net/npm/chart.js"></script>

<!-- <%= JSON.stringify(dataAnalysis) %> -->

<script>
        
  let Money = "<%= dataMoney %>"
  let Name = "<%= dataName %>"

  let dataMoney = Money.split(",");
  let dataName = Name.split(",");
  
  const ctx = document.getElementById('myChart');
  
  new Chart(ctx, {
    type: 'doughnut',
    data: {
      labels: dataName,
      datasets: [{
      label: 'Transaction',
      data: dataMoney,
      fill: false,
      backgroundColor: [
      'rgb(255, 99, 132)',
      'rgb(54, 162, 235)',
      'rgb(255, 205, 86)',
      'rgb(50, 168, 82)',
      'rgb(213, 146, 240)'

    ],
      tension: 0.1
    }]
    },
    options: {
      scales: {
        y: {
          beginAtZero: true
        }
      }
    }
  });
</script>


    
    <div>


    </div>

    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-C6RzsynM9kWDrMNeT87bh95OGNyZPhcTNXj1NW7RuBCsyN/o0jlpcV8Qyq46cDfL" crossorigin="anonymous"></script>
  </body>
</html>